

struct TypeInfo {
    string name;
}

struct Struct {
    char* name;
    StructField[] fields;
}

struct StructField {
    char* name;
    uint32 typeid;
    uint32 offset;
}

struct Procedure {
    string name;
    void* code;

    TypeInfo return_type;
    TypeInfo[] argument_types;
    string[]   argument_names;

}

